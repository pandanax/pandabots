# PostgreSQL Configuration
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD_HERE

# n8n Configuration
N8N_HOST=n8n.mandala-app.online
TIMEZONE=Europe/Moscow

# ВАЖНО: Сгенерируйте криптографически стойкий ключ!
# Используйте: openssl rand -hex 32
N8N_ENCRYPTION_KEY=CHANGE_ME_GENERATE_WITH_OPENSSL_RAND_HEX_32

# Логирование
N8N_LOG_LEVEL=info

# Email для Let's Encrypt (укажите ваш реальный email)
LETSENCRYPT_EMAIL=your-email@example.com
